<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.misde.mapper.ArticleMapper">
	<select id="findArticles" resultType="cn.misde.model.Article">
		select articleId,creatDatetime,modifyDatetime,articleTitle,articleContent,imageSrc
		from article;
	</select>
</mapper>
